{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 3,
      "maxLength": 255,
      "private": true,
      "required": true
    },
    "dni": {
      "type": "string",
      "required": true,
      "minLength": 10,
      "maxLength": 10,
      "private": true
    },
    "phone": {
      "type": "biginteger",
      "required": true,
      "min": "9999999999",
      "max": "9999999999"
    },
    "address": {
      "type": "richtext",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive"
      ],
      "required": true
    }
  }
}
